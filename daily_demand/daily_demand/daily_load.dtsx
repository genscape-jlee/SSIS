                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  expressionType="Notify"
                  name="SqlCommand">select 'CAISO' area ,cast(CONVERT(VARCHAR(10), dateentered, 121) as datetime)gdate,cast(CONVERT(VARCHAR(10), dateentered, 121) as datetime)estdate,
cast(substring(CONVERT(VARCHAR(19), dateentered, 120),12,2) as numeric) hour,cast(substring(CONVERT(VARCHAR(19), dateentered, 120),12,2) as numeric) esthour,
value as "load", getdate() as time_stamp,null as estimated
from tblData a where sensorid in (22503)
and dateentered &gt;= CONVERT(date, getDate()-1) and dateentered &lt; CONVERT(date, getDate())
union all 
select 'CAPGE' area ,cast(CONVERT(VARCHAR(10), dateentered, 121) as datetime)gdate,cast(CONVERT(VARCHAR(10), dateentered, 121) as datetime)estdate,
cast(substring(CONVERT(VARCHAR(19), dateentered, 120),12,2) as numeric) hour,cast(substring(CONVERT(VARCHAR(19), dateentered, 120),12,2) as numeric) esthour,
value as "load", getdate() as time_stamp,null as estimated
from tblData a where sensorid in (22504)
and dateentered &gt;= CONVERT(date, getDate()-1) and dateentered &lt; CONVERT(date, getDate())
union all
select 'CASCE' area ,cast(CONVERT(VARCHAR(10), dateentered, 121) as datetime)gdate,cast(CONVERT(VARCHAR(10), dateentered, 121) as datetime)estdate,
cast(substring(CONVERT(VARCHAR(19), dateentered, 120),12,2) as numeric) hour,cast(substring(CONVERT(VARCHAR(19), dateentered, 120),12,2) as numeric) esthour,
value as "load", getdate() as time_stamp,null as estimated
from tblData a where sensorid in (22505)
and dateentered &gt;= CONVERT(date, getDate()-1) and dateentered &lt; CONVERT(date, getDate())
union all
select 'CASDGE' area ,cast(CONVERT(VARCHAR(10), dateentered, 121) as datetime)gdate,cast(CONVERT(VARCHAR(10), dateentered, 121) as datetime)estdate,
cast(substring(CONVERT(VARCHAR(19), dateentered, 120),12,2) as numeric) hour,cast(substring(CONVERT(VARCHAR(19), dateentered, 120),12,2) as numeric) esthour,
value as "load", getdate() as time_stamp,null as estimated
from tblData a where sensorid in (22506)
and dateentered &gt;= CONVERT(date, getDate()-1) and dateentered &lt; CONVERT(date, getDate())
union all--MISO comes in GW so it needs to be converted to MW
select 'MISO' area ,cast(CONVERT(VARCHAR(10), dateentered, 121) as datetime)gdate,cast(CONVERT(VARCHAR(10), dateentered, 121) as datetime)estdate,
cast(substring(CONVERT(VARCHAR(19), dateentered, 120),12,2) as numeric) hour,cast(substring(CONVERT(VARCHAR(19), dateentered, 120),12,2) as numeric) esthour,
value*1000 as "load", getdate() as time_stamp,null as estimated
from tblData a where sensorid in (6958)
and dateentered &gt;= CONVERT(date, getDate()-1) and dateentered &lt; CONVERT(date, getDate())
union all
